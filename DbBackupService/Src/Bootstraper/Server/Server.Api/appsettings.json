{
  "NotifyServiceSettings":
  {
    "Env": "Dev",
    "Scheme": "https",
    "DevelopmentPort": 5000,
    "ProductionHost": "production.example.com"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "System.Net.Http": "Warning",
      "System.Net.Http.HttpClient": "Warning"
    }
  },
  "AuthSettings": {
    "AutoConfirmAccount": true,
    "DefaultTokenExpirationTimeInMinutes": 480,
    "EnableRegisterModule": true,
    "MainAdminEmailAddress": "admin@octo.com"
  },
  "AllowedHosts": "*",
  "AllowedOrigins": "https://localhost:10443, https://localhost:10000, https://localhost",
  "https_port": 443,
  "SeqConfiguration": {
    "ApiKey": "",
    "Host": ""
  }
}
